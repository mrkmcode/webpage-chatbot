{
  "name": "webpage-chatbot",
  "version": "1.0.0",
  "description": "A chatbot made for webpage using the MERN stack and powered by Google's DialogFlow.",
  "main": "index.js",
  "engines": {
    "node": "12.16.2",
    "npm": "6.14.4"
  },
  "scripts": {
    "start": "node index.js",
    "start:backend": "nodemon index.js",
    "start:frontend": "npm run start --prefix client",
    "start:dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Rohit Markam",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dialogflow": "^1.2.0",
    "express": "^4.17.1",
    "structjson": "^1.0.1",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
}
